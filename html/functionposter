<script>

    function distance(itemCost, moneySpent){
        var value$ = 1
        var valueQ = 0.25
        var valueDi = 0.10
        var valueN = 0.05
        var valueP = 0.01

        var change = moneySpent - itemCost;
        var Dchange = Math.floor(change / value$);
        change = change - (Dchange*value$);
        console.log("Dollars : " + Dchange);
        var Qchange = Math.floor(change / valueQ);
         change = change - (Qchange*valueQ);
        console.log("Quarters : " + Qchange);
        var Dichange = Math.floor(change / valueDi);
         change = change - (Dichange*valueDi);
        console.log("Dime : " + Dichange)
        var Nchange = Math.floor(change / valueN);
         change = change - (Nchange*valueN);
        console.log("Nickels : " + Nchange);
         var Pchange = Math.round(change / valueP);
         change = change - (Pchange*valueP);
        console.log("Pennies : " + Pchange);
    }
    
    var prompt1 = prompt("How much the item costs?");
    var prompt2 = prompt("How much you are spending?");
    
   
                      
   distance(prompt1, prompt2);

   
                      

</script>
