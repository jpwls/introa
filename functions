<script>
 //sqrt(e-s)/g
unction distance(eMileattheend, sMilesatthestart, gUsedgallons) {
var eSide = parseInt((eMileattheend - sMilesatthestart) / gUsedgallons);
alert("The result is: " + eSide);
}
var prompt1 = parseInt(prompt("Please give me a e value."));
var prompt2 = parseInt(prompt("Please give me a s value"));
var prompt3 = parseInt(prompt("Please give me a g value."));
  distance(prompt1, prompt2, prompt3);
</script>
